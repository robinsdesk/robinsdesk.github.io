# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /mammal/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /mammal/index.php [L]
</IfModule>

<IfModule mod_substitute.c>
SubstituteMaxLineLength 10M
</IfModule>

# END WordPress